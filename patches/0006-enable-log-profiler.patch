+import { AOTProfilerOptions, BrowserProfilerOptions, LogProfilerOptions } from "./types";
 import cwraps from "./cwraps";
 import { MonoMethod } from "./types";
 
@@ -98,3 +98,8 @@ export function mono_wasm_profiler_leave(method: MonoMethod): void {
